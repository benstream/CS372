<style>
    table,
    th,
    td {
        border: 1px solid;
    }
</style>

<h1>Search Results 📊</h1>

<div id="navigation"><a href="/success">Home</a> — <a href="/logout">Logout</a></div>

<br />

<table>
    <tr>
        <th>Title</th>
        <th>Thumbnail</th>
        <th>Rating</th>
    </tr>

    <% results.forEach(function(results) { %>
        <tr>
            <td>
              <a href="/content"> <%= results.title %></a> </td>
            <td><img src="https://img.youtube.com/vi/<%=results.video%>/default.jpg" alt="" border="3" height="180" width="320"></img></th>
            <td><%= results.rating %> / 5 ⭐️</td>
        </tr>
    <% }); %>
</table>
