<style>
    table,
    th,
    td {
        border: 1px solid;
    }
</style>

<h1>Movie Dashboard 📊</h1>

<div id="navigation"><a href="/success">Home</a> ~ <a href="/">Logout</a></div>

<br />

<table>
    <tr>
        <th>Title</th>
        <th>Review</th>
        <th>Rating</th>
    </tr>

    <% movies.forEach(function(movie) { %>
        <tr>
            <td><%= movie.title %></td>
            <td><%= movie.review %></td>
            <td><%= movie.rating %> / 5 ⭐️</td>
        </tr>
    <% }); %>
</table>
